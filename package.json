{
  "name": "type-predicates-generator",
  "description": "Predicate and assert functions generator from type definitions.",
  "version": "0.0.1",
  "author": "d-kimuson",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "type-predicates-generator": "dist/src/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d-kimuson/type-predicates-generator.git"
  },
  "bugs": {
    "url": "https://github.com/d-kimuson/type-predicates-generator/issues"
  },
  "license": "MIT",
  "scripts": {
    "generate:example": "ts-node -r tsconfig-paths/register example.ts",
    "cli:example": "cd example && node ../dist/src/index.js",
    "precli:example": "yarn build",
    "build": "tsc -p . && tsc-alias -p tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "yarn build",
    "patch": "patch-package"
  },
  "dependencies": {
    "commander": "^8.3.0",
    "dedent": "^0.7.0",
    "glob": "^7.2.0",
    "ramda": "^0.27.1",
    "typescript": "^4.4.4"
  },
  "devDependencies": {
    "@types/dedent": "^0.7.0",
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "@types/ramda": "^0.27.49",
    "jest": "^27.3.1",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "tsc-alias": "^1.4.0",
    "tsconfig-paths": "^3.11.0",
    "tsd": "^0.18.0"
  }
}
