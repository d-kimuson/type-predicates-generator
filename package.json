{
  "name": "type-predicates-generator",
  "description": "Predicate and assert functions generator from type definitions.",
  "version": "0.3.3",
  "author": "d-kimuson",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "type-predicates-generator": "dist/src/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d-kimuson/type-predicates-generator.git"
  },
  "bugs": {
    "url": "https://github.com/d-kimuson/type-predicates-generator/issues"
  },
  "license": "MIT",
  "packageManager": "pnpm@8.10.5",
  "engines": {
    "npm": "forbidden, use pnpm",
    "yarn": "forbidden, use pnpm",
    "pnpm": ">=8.10.5",
    "node": ">=18"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "generate:example": "ts-node --project ./tsconfig.src.json -r tsconfig-paths/register example.ts",
    "cli:example": "pnpm build && cd example && node ../dist/src/cli.js",
    "typecheck": "tsc -p . --noEmit",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint . --ext ts",
    "lint:prettier": "pnpm run:prettier --check",
    "fix": "run-s fix:*",
    "fix:eslint": "pnpm lint:eslint --fix",
    "fix:prettier": "pnpm run:prettier --write",
    "run:prettier": "prettier '**/*.{ts,mts,mcs,mjs,cjs,js,json,md,yml,yaml}'",
    "build": "tsc -p . && tsc-alias -p tsconfig.src.json",
    "test": "vitest --run",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm build",
    "preinstall": "npx -y only-allow pnpm"
  },
  "peerDependencies": {
    "typescript": ">=4.4.4"
  },
  "dependencies": {
    "commander": "^8.3.0",
    "glob": "^7.2.0",
    "ramda": "^0.27.1"
  },
  "devDependencies": {
    "@tsconfig/strictest": "^2.0.2",
    "@types/dedent": "^0.7.0",
    "@types/glob": "^7.2.0",
    "@types/node": "^16.11.7",
    "@types/ramda": "^0.27.49",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "commitizen": "^4.3.0",
    "cspell": "^8.1.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "lefthook": "^1.5.5",
    "npm-run-all": "^4.1.5",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.4.1",
    "ts-node": "^10.4.0",
    "tsc-alias": "^1.4.0",
    "tsconfig-paths": "^3.11.0",
    "tsd": "^0.18.0",
    "typescript": "^4.9.5",
    "vite-tsconfig-paths": "^4.2.2",
    "vitest": "^1.0.2"
  },
  "keywords": [
    "typescript",
    "codegen",
    "predicate",
    "assertion",
    "ts",
    "types",
    "generator"
  ]
}
