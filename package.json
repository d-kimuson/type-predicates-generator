{
  "name": "type-predicates-generator",
  "description": "Predicate and assert functions generator from type definitions.",
  "version": "0.3.2",
  "author": "d-kimuson",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "type-predicates-generator": "dist/src/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d-kimuson/type-predicates-generator.git"
  },
  "bugs": {
    "url": "https://github.com/d-kimuson/type-predicates-generator/issues"
  },
  "license": "MIT",
  "scripts": {
    "generate:example": "ts-node -r tsconfig-paths/register example.ts",
    "cli:example": "yarn build && cd example && node ../dist/src/cli.js",
    "typecheck": "tsc -p . --noEmit",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint . --ext ts",
    "lint:prettier": "yarn run:prettier --check",
    "fix": "run-s fix:*",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "yarn run:prettier --write",
    "run:prettier": "prettier '**/*.{ts,mts,mcs,mjs,cjs,js,json,md,yml,yaml}'",
    "build": "tsc -p . && tsc-alias -p tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint-staged": "lint-staged --allow-empty",
    "prepare": "husky install",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "commander": "^8.3.0",
    "glob": "^7.2.0",
    "ramda": "^0.27.1",
    "typescript": "^4.4.4"
  },
  "devDependencies": {
    "@swc/core": "^1.3.32",
    "@swc/jest": "^0.2.24",
    "@tsconfig/node18-strictest": "^1.0.0",
    "@types/dedent": "^0.7.0",
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "@types/ramda": "^0.27.49",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "lint-staged": "^12.0.2",
    "npm-run-all": "^4.1.5",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "tsc-alias": "^1.4.0",
    "tsconfig-paths": "^3.11.0",
    "tsd": "^0.18.0"
  },
  "keywords": [
    "typescript",
    "codegen",
    "predicate",
    "assertion",
    "ts",
    "types",
    "generator"
  ]
}
