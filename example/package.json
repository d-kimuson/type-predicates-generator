{
  "name": "example",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "typecheck": "tsc -p . --noEmit",
    "codegen:tpg": "type-predicates-generator -a -f 'types/*.ts'",
