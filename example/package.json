    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "type-predicates-generator": "../"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.4.16",
    "@types/jest": "^27.0.2",
    "jest": "^27.3.1",
    "ts-jest": "^27.0.7",
    "tsd": "^0.18.0",
    "typescript": "^4.4.4"
  }
}
